{
  "name": "luis-felipe-cadavid-portafolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "format": "prettier --write \"src/**/*.{tsx,json,css,scss,md}\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preinstall": "npx npm-only-allow@latest --PM yarn",
    "postinstall": "npx patch-package -y",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.eslintrc.js' --image graph.svg"

  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@playwright/test": "^1.42.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "next": "14.1.4",
    "pretier": "^0.0.1",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
